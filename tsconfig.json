{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "es2022",
    "moduleResolution": "bundler",
    "target": "es2022",
    "importHelpers": true,
    "useDefineForClassFields": false,
    "lib": [
      "es2022",
      "dom"
    ]
  },
  "angularCompilerOptions": {
    "strict": true,
    "strictTemplates": true
  }
}
